{
  "domain": "mystrom_switch",
  "name": "myStrom Switch",
  "version": "1.0.0",
  "documentation": "https://github.com/proBieri/HAmyStrom",
  "issue_tracker": "https://github.com/proBieri/HAmyStrom/issues",
  "requirements": ["aiohttp>=3.8.0"],
  "codeowners": ["@proBieri"],
  "config_flow": true,
  "iot_class": "local_polling",
  "integration_type": "device",
  "loggers": ["mystrom"],
  "zeroconf": [
    {
      "type": "_hap._tcp.local.",
      "name": "mystrom-switch*"
    }
  ],
  "dhcp": [
    {
      "hostname": "mystrom-switch*",
      "macaddress": "64B473*"
    }
  ]
}
